{
  "family": "latest-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "my-container3",
      "image": "559050231342.dkr.ecr.ap-south-1.amazonaws.com/ecs-repo:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::559050231342:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::559050231342:role/cluster-manage"
}
